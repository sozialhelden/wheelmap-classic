{
  "name": "wheelmap",
  "version": "1.0.0",
  "license": "AGPL-3.0",
  "devDependencies": {
    "babel": "^6.3.13",
    "babel-jest": "^6.0.1",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-runtime": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "gulp": "^3.9.0",
    "gulp-util": "^3.0.7",
    "immutable": "^3.7.5",
    "jest-cli": "^0.8.2",
    "mout": "^0.11.0",
    "react": "^0.14.5",
    "react-addons-test-utils": "^0.14.3",
    "react-dom": "^0.14.5",
    "react-redux": "^4.0.0",
    "react-select": "^0.8.2",
    "redux": "^3.0.4",
    "redux-thunk": "^1.0.0",
    "require-dir": "^0.3.0",
    "reselect": "^2.0.1",
    "vinyl-source-stream": "^1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sozialhelden/wheelmap.git"
  },
  "engines": {
    "node": ">= 4.1"
  },
  "scripts": {
    "test": "node_modules/.bin/jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/react-redux",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/redux",
      "<rootDir>/node_modules/redux-thunk",
      "<rootDir>/node_modules/mout",
      "<rootDir>/node_modules/hoist-non-react-statics"
    ]
  },
  "babel": {
    "plugins": [
      "transform-class-properties"
    ],
    "presets": [
      "es2015",
      "react"
    ]
  }
}
